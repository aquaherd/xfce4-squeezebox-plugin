<?xml version="1.0"?>
<interface>
  <requires lib="gtk+" version="2.16"/>
  <!-- interface-naming-policy project-wide -->
  <object class="GtkWindow" id="theme">
    <property name="type">popup</property>
    <property name="default_width">72</property>
    <property name="default_height">23</property>
    <property name="type_hint">toolbar</property>
    <child>
      <object class="GtkHBox" id="root">
        <property name="visible">True</property>
        <child>
          <object class="GtkButton" id="btnPrev">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="relief">none</property>
            <child>
              <object class="GtkImage" id="imgPrev">
                <property name="visible">True</property>
                <property name="pixel_size">16</property>
                <property name="icon_name">media-seek-backward</property>
              </object>
            </child>
          </object>
          <packing>
            <property name="position">0</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="btnPlayPause">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="relief">none</property>
            <child>
              <object class="GtkImage" id="imgPlayPause">
                <property name="visible">True</property>
                <property name="pixel_size">16</property>
                <property name="icon_name">media-playback-start</property>
              </object>
            </child>
          </object>
          <packing>
            <property name="position">1</property>
          </packing>
        </child>
        <child>
          <object class="GtkButton" id="btnNext">
            <property name="visible">True</property>
            <property name="can_focus">True</property>
            <property name="receives_default">True</property>
            <property name="relief">none</property>
            <child>
              <object class="GtkImage" id="imgNext">
                <property name="visible">True</property>
                <property name="pixel_size">16</property>
                <property name="icon_name">media-seek-forward</property>
              </object>
            </child>
          </object>
          <packing>
            <property name="position">2</property>
          </packing>
        </child>
      </object>
    </child>
  </object>
</interface>
